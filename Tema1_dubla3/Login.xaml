<Window x:Class="Tema1_dubla3.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema1_dubla3"
        mc:Ignorable="d"
        Title="Login" Height="303" Width="490">
    <Grid Margin="0,0,0,-6">
        <Label Content="User ID:" HorizontalAlignment="Left" Margin="39,31,0,0" VerticalAlignment="Top" Height="42" Width="109" FontSize="20"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="39,73,0,0" VerticalAlignment="Top" Height="53" Width="109" FontSize="20"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="116,147,0,0" VerticalAlignment="Top" Click="Button_Click" Height="38" Width="96" FontSize="18"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="231,146,0,0" VerticalAlignment="Top" Click="Button_Click_1" Height="39" Width="110" FontSize="18"/>
        <TextBox HorizontalAlignment="Left" Name="userIDTextBox" Margin="148,31,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="224" TextChanged="TextBox_TextChanged" Height="37" FontSize="18"/>
        <PasswordBox HorizontalAlignment="Left" Name="passwordTextBox" Margin="148,78,0,0" VerticalAlignment="Top" Width="224" Height="34"/>
    </Grid>
</Window>
